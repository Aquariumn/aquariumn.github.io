    <svg:svg width="100%" height="100%" version="1.1" style="position:fixed; top:0; left:0; z-index:-1">
      <!-- Import shrimp SVG and convert to monochrome pattern -->
      <svg:defs>
        <!-- Monochrome filter -->
        <svg:filter id="monochrome">
          <svg:feColorMatrix type="matrix" values="                   0.33 0.33 0.33 0 0                   0.33 0.33 0.33 0 0                   0.33 0.33 0.33 0 0                   0    0    0    1 0"/>
          <svg:feComponentTransfer>
            <svg:feFuncR type="table" tableValues="0.2 0.6"/>
            <svg:feFuncG type="table" tableValues="0.2 0.7"/>
            <svg:feFuncB type="table" tableValues="0.4 0.9"/>
          </svg:feComponentTransfer>
        </svg:filter>
        <!-- Shrimp pattern -->
        <svg:pattern id="shrimpPattern" patternUnits="userSpaceOnUse" width="100" height="100" patternTransform="rotate(45)">
          <svg:rect width="100" height="100" fill="#63639C"/>
          <!-- External shrimp SVG reference with monochrome filter applied -->
          <svg:image xlink:href="shrimp.svg" x="10" y="10" width="80" height="80" filter="url(#monochrome)" opacity="0.2"/>
        </svg:pattern>
      </svg:defs>
      <svg:rect width="100%" height="100%" fill="url(#shrimpPattern)"/>
    </svg:svg>
